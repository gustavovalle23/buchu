{
  "name": "buchu",
  "version": "0.0.1",
  "description": "Uniform, auditable and secure use case javascript library",
  "main": "./src/buchu.js",
  "scripts": {
    "test": "mocha --timeout 999999 --colors --recursive ./test",
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
    "semantic-release": "semantic-release"
  },
  "directories": {
    "lib": "src",
    "test": "test",
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herbsjs/buchu.git"
  },
  "bugs": {
    "url": "https://github.com/herbsjs/buchu/issues/"
  },
  "engines": {
    "node": ">= 12.3.0"
  },
  "keywords": [
    "buchu",
    "usecase",
    "clean architecture"
  ],
  "author": "David Lojudice Sb",
  "homepage": "https://github.com/herbsjs/buchu#readme",
  "license": "MIT",
  "dependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "suma": "^1.0.10",
    "uuid": "^8.3.1"
  },
  "optionalDependencies": {
    "gotu": "^1.0.20"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "semantic-release": "^17.4.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "semantic_version"
  }
}
