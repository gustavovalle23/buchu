{
  "name": "@herbsjs/buchu",
  "version": "1.3.1",
  "description": "Uniform, auditable and secure use case javascript library",
  "main": "./src/buchu.js",
  "scripts": {
    "test": "mocha --timeout 999999 --colors --recursive ./test",
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "directories": {
    "lib": "src",
    "test": "test",
    "example": "examples"
  },
  "bugs": {
    "url": "https://github.com/herbsjs/buchu/issues/"
  },
  "engines": {
    "node": ">= 12.3.0"
  },
  "keywords": [
    "buchu",
    "usecase",
    "clean architecture"
  ],
  "author": "David Lojudice Sb",
  "homepage": "https://github.com/herbsjs/buchu#readme",
  "license": "MIT",
  "dependencies": {
    "@herbsjs/suma": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "optionalDependencies": {
    "@herbsjs/gotu": "^1.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "babel-eslint": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herbsjs/buchu.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
